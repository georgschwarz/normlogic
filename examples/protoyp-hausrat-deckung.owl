    <!-- http://www.faktorlogik.de/ontologie/prototyp_hausrat/core.owl#§1.1.a_VHB -->

    <owl:Thing rdf:about="&core2;§1.1.a_VHB">
        <rdf:type rdf:resource="&core2;Klausel"/>
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdfs:label xml:lang="de">1.1 a) VHB Versicherungsfall (Brand, Blitzschlag etc.)</rdfs:label>
        <rdfs:comment xml:lang="de">Der Versicherer leistet Entschädigung für versicherte Sachen, die durch
a)       Brand, Blitzschlag, Explosion, Implosion, Anprall oder Absturz eines Luftfahrzeuges,
         seiner Teile oder seiner Ladung,
...
zerstört oder beschädigt werden oder infolgedessen abhanden kommen.</rdfs:comment>
    </owl:Thing>
    


    <!-- http://www.faktorlogik.de/ontologie/prototyp_hausrat/core.owl#§1.1.b_VHB -->

    <owl:Thing rdf:about="&core2;§1.1.b_VHB">
