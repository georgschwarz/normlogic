<?xml version="1.0" encoding="UTF-8"?>
<plugin>

   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            name="org.normlogic.navigator"
            application="org.eclipse.e4.ui.workbench.swt.E4Application">
         <property
               name="applicationCSS"
               value="platform:/plugin/org.normlogic.navigator.app/css/default.css">
         </property>
         <property
               name="appName"
               value="org.normlogic.navigator">
         </property>
         <property
               name="preferenceCustomization"
               value="plugin_customization.ini">
         </property>
         <property
               name="windowImages"
               value="icons/16/graph.png">
         </property>
      </product>
   </extension>
   <extension
         point="org.eclipse.ui.decorators">
      <decorator
            class="org.normlogic.navigator.parts.LibraryPart$LibraryLabelProvider"
            icon="icons/8/important.png"
            id="org.normlogic.navigator.app.library"
            label="important"
            lightweight="true"
            location="BOTTOM_RIGHT">
      </decorator>
   </extension>
   <extension
         id="Hansi"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.eclipse.ant.core.antRunner"
            name="org.normlogic.navigator">
         <property
               name="windowImages"
               value="icons/16/graph.png">
         </property>
      </product>
   </extension>

</plugin>
